            </ul>
          </div>
        </div>
      </div>
  </div>
</div>
<!-- HEADER END -->

<!-- Login -->
<div class="container myform">
	<div class="row">
		<div class="col-md-6 col-md-push-3">
			<div class="card">
				<form role="form" action="" method="post">
					<img class="mylogo" src="./assets/user/img/logo.png" alt="logo">
					<div class="form-group">
						<label for="">User Name</label>
						<input type="text" class="form-control" name="username" placeholder="Input field">
					</div>

					<div class="form-group">
						<label for="">Password</label>
						<input type="password" class="form-control" name="password" placeholder="Input field">
					</div>
					<button class="btn btn-primary" style="text-align: center">Submit</button>
					
					{{#if showError}}
						<div class="alert alert-danger" style="margin-top: 15px">
							<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
							<strong>{{showError}}</strong> 
						</div>
					{{/if}}
				</form>
			</div>
		</div>
	</div>
</div>

<!-- End Login -->

